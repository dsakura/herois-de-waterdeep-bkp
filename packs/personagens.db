Bonus":0,"chatFlavor":"","critical":null,"damage":{"parts":[],"versatile":""},"formula":"","save":{"ability":"","dc":null,"scaling":"flat"},"requirements":"","recharge":{"value":null,"charged":false},"attributes":{"spelldc":10}},"sort":13500000,"flags":{},"img":"modules/plutonium/media/icon/mighty-force.svg","effects":[],"folder":null,"permission":{"default":0}},{"_id":"aR8ycTUKucClf5Ij","name":"Tool Proficiency","type":"feat","data":{"description":{"value":"<div  class=\"rd__b  rd__b--3\"><p>You gain proficiency with the artisan's tools of your choice: <a href=\"https://5e.tools/items.html#smith's%20tools_phb\"  onmouseover=\"Renderer.hover.pHandleLinkMouseOver(event, this, 'items.html', 'phb', 'smith\\'s%20tools_phb', null)\" onmouseleave=\"Renderer.hover.handleLinkMouseLeave(event, this)\" onmousemove=\"Renderer.hover.handleLinkMouseMove(event, this)\"  ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\" >Smith's tools</a>, <a href=\"https://5e.tools/items.html#brewer's%20supplies_phb\"  onmouseover=\"Renderer.hover.pHandleLinkMouseOver(event, this, 'items.html', 'phb', 'brewer\\'s%20supplies_phb', null)\" onmouseleave=\"Renderer.hover.handleLinkMouseLeave(event, this)\" onmousemove=\"Renderer.hover.handleLinkMouseMove(event, this)\"  ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\" >brewer's supplies</a>, or <a href=\"https://5e.tools/items.html#mason's%20tools_phb\"  onmouseover=\"Renderer.hover.pHandleLinkMouseOver(event, this, 'items.html', 'phb', 'mason\\'s%20tools_phb', null)\" onmouseleave=\"Renderer.hover.handleLinkMouseLeave(event, this)\" onmousemove=\"Renderer.hover.handleLinkMouseMove(event, this)\"  ontouchstart=\"Renderer.hover.handleTouchStart(event, this)\" >mason's tools</a>.</p><div class=\"rd__spc-inline-post\"></div></div>","chat":"","unidentified":""},"source":"PHB","activation":{"type":"","cost":0,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":""},"range":{"va